<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Login</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/login.css">
    <script src="/javascripts/jquery.1.10.2.min.js"></script>
    <script src="/javascripts/jquery.cookie.js"></script>

</head>

<body>
    <div class="container">
        <div class="login">
            <!-- 登录界面表单 -->
            <h1>Welcome!</h1>
            <form action="" id = 'formData'>
                <div class="input-group cell">
                    <span class="input-group-addon" id="basic-addon1">账号：</span>
                    <input type="text" class="form-control" placeholder="Username" aria-describedby="basic-addon1" name="account">
                </div>
                <div class="input-group cell">
                    <span class="input-group-addon" id="basic-addon1">密码：</span>
                    <input type="password" class="form-control" placeholder="Password" aria-describedby="basic-addon1" name="password">
                </div>
                <div class="button-group ">
                    <input class="btn btn-primary login-btn cf" type="submit" id="login" value="submit">Login</input>
                </div>
                <hr/>
            </form>
        </div>
    </div>
</body>
<script src="/javascripts/login.js"></script>
</html>
